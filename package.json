    "jsonwebtoken": "^8.5.1",
    "line-awesome": "^1.0.2",
    "mongoose": "^5.6.5",
    "morgan": "^1.9.1",
    "npm-run-all": "^4.1.5",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
